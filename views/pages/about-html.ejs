                    <div class="intro-text text-center">
                        <h1>公司发展历史</h1>
                        <div class="blog-thumb mt-40">
                            <img src="<%= company && company.history %>" width="100%"
                                 alt="">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- intro-area end -->
