<!-- wrapper-box start -->
<div class="wrapper-box p-0">
    <!-- page-title-area start -->
    <div class="page-title-area pt-210 pb-210"
         data-background="https://nmist.oss-cn-hangzhou.aliyuncs.com/img/bg/page-title.jpg">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-12">
                    <div class="page-title text-center">
                        <h1>天磁信息科技</h1>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb justify-content-center">
                                <li class="breadcrumb-item">
                                    公司简介
                                </li>
                                <li class="breadcrumb-item">
                                    发展历史
                                </li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- page-title-area end -->

    <!-- who-we-area start -->
    <div class="who-we-area cover-bottom pt-100">
        <div class="container">
            <div class="row">
                <div class="col-xl-8 col-lg-8 wh-pad pr-60">
                    <div class="who-we-box white-bg">
                        <h2 class="mb-20">公司简介</h2>
                        <% company && company.introduction && company.introduction.split("||").forEach(function (item, index) { %>
                        <p class="suojin"><%= item %></p>
                        <% }) %>
                        <ul class="expert-list clearfix mb-30">
                            <% company && company.character && company.character.split("||").forEach(function (item, index) { %>
                            <li><%= item %></li>
                            <% }) %>
                        </ul>
                        <a data-animation="fadeInLeft" data-delay="1.2s" href="/contact" class="btn brand-btn"
                           style="animation-delay: 1.2s;"
                           tabindex="0">联系我们
                            <span>→</span>
                        </a>
                    </div>
                </div>
                <div class="col-xl-4 col-lg-4">
                    <div class="zoomImage"
                         style="background-image: url(<%= company && company.imgUrl %>)"></div>
                </div>
            </div>
        </div>
    </div>
    <!-- who-we-area end -->
    <!-- intro-area start -->
    <div class="intro-area pt-50 pb-50 gray-bg">
        <div class="container">
