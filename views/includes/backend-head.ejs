    <style type="text/css">
        /* + Pagination */
        .ow-pagination {
            display: inline-block;
            margin-bottom: 50px;
            text-align: left;
            width: 100%;
        }

        .ow-pagination ul {
            padding-left: 0;
            margin: 0;
        }

        .ow-pagination ul li a {
            display: inline-block;
        }

        .ow-pagination ul li {
            display: inline-block;
        }

        .ow-pagination .pagination > li:first-child > a,
        .ow-pagination .pagination > li:last-child > a {
            background-color: #fff;
            color: #232323;
            border-radius: 0;
        }

        .ow-pagination ul li a {
            background-color: #fff;
            border: 1px solid #e2e2e2;
            border-radius: 0;
            color: #333;
            font-size: 12px;
            height: 28px;
            vertical-align: middle;
            padding: 0 15px;
            letter-spacing: 0.52px;
            line-height: 28px;
            position: relative;
            text-align: center;
            transition: all 1s ease 0s;
            text-transform: uppercase;
        }

        .ow-pagination ul li a:hover {
            color: #186fbe;
        }

        .ow-pagination ul li.active a {
            background-color: #337ab7;
            border-color: #337ab7;
        }

        .ow-pagination ul li.disabled a {
            cursor: not-allowed;
            border-color: #ddd;
            background-color: #fff;
            color: #777;
        }
    </style>
</head>